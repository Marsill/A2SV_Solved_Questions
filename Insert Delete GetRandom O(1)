class RandomizedSet:

    def __init__(self):
        self.num = {}
        self.pos = []

    def insert(self, val: int) -> bool:
        res = val not in self.num
        if res:
            self.num[val] = len(self.pos)
            self.pos.append(val)
        return res
    def remove(self, val: int) -> bool:
        res = val in self.num
        if res:
            idx = self.num[val]
            lastval = self.pos[-1]
            self.pos[idx] = lastval
            self.pos.pop()
            self.num[lastval] = idx
            del self.num[val]
         
        return res

    def getRandom(self) -> int:
        return random.choice(self.pos)


# Your RandomizedSet object will be instantiated and called as such:
# obj = RandomizedSet()
# param_1 = obj.insert(val)
# param_2 = obj.remove(val)
# param_3 = obj.getRandom()
