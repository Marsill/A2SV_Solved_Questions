def swap_case(s):
    result = []
    for ch in s:
        if ch.isalpha():
            if ch.isupper():
                ch = ch.lower()
            else:
                ch = ch.upper()
        
        result.append(ch)
        
    return ''.join(result)

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
