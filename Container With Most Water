class Solution:
    def maxArea(self, height: List[int]) -> int:
        i, j = 0, len(height)-1
        container = 0

        while i < j:
            new = (j-i) * min(height[i], height[j])
            container = max(new, container)
            
            if height[i] < height[j]:
                i += 1
            else:
                j -= 1
        return container
