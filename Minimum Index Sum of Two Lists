class Solution:
    def findRestaurant(self, list1: List[str], list2: List[str]) -> List[str]:
        item_dict = {item: idx for idx, item in enumerate(list1)}
        result = {}
        for ind, s in enumerate(list2):
            var = 0
            if s in item_dict:
                var = ind + item_dict[s]
                result[s] = var
        min_value = min(result.values())
        min_key = [k for k, v in result.items() if v == min_value]
        return min_key
