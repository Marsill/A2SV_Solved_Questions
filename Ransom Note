class Solution:
    def canConstruct(self, ransomNote: str, magazine: str) -> bool:
        r = Counter(ransomNote)
        m = Counter(magazine)

        for ch in ransomNote:
            if ch in m:
                m[ch] -= 1
                r[ch] -= 1
                if r[ch] == 0:
                    del r[ch]
                if m[ch] == 0:
                    del m[ch]
        if not r:
            return True
        else:
            return False
