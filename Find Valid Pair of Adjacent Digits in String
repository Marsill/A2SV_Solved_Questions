class Solution:
    def findValidPair(self, s: str) -> str:
        n = [int(ch) for ch in s ]
        result = ""
        num = Counter(n)
        for i in range(len(n)-1):
            if n[i] != n[i+1]:
                if num[n[i]] == n[i] and num[n[i+1]] == n[i+1]:
                    result = result + str(n[i])
                    result = result + str(n[i+1])
                    del num[n[i]]
                    del num[n[i+1]]
            if len(result) == 2:
                break
        return result
