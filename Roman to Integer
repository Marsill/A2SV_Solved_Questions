class Solution:
    def romanToInt(self, s: str) -> int:
        roman = {'I':1, 'IV': 4, 'V': 5, 'IX': 9, 'X': 10, 'XL': 40, 'L':50, 'XC':90, 'C': 100, 'CD': 400, 'D':500, 'CM':900, 'M':1000}
        result = 0
        for i in range(len(s)):
            if (i+1) < len(s) and  roman[s[i]] < roman[s[i +1]]:
                result -=  roman[s[i]]    
            else:
                result += roman[s[i]]
                
        return result
